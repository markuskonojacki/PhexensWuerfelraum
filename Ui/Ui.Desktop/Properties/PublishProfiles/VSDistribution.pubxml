<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Configuration>Release</Configuration>
    <Platform>Any CPU</Platform>
    <PublishDir>C:\VSDistribution\PhexensWuerfelraum.Ui.Desktop.Publish</PublishDir>
    <PublishProtocol>FileSystem</PublishProtocol>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <SelfContained>true</SelfContained>
    <PublishSingleFile>True</PublishSingleFile>
    <PublishReadyToRun>True</PublishReadyToRun>
    <PublishTrimmed>False</PublishTrimmed>
    <DeleteExistingFiles>True</DeleteExistingFiles>
    <IncludeNativeLibrariesForSelfExtract>True</IncludeNativeLibrariesForSelfExtract>
    <DebugType>None</DebugType>
    <CopyOutputSymbolsToPublishDirectory>false</CopyOutputSymbolsToPublishDirectory>

    <ExcludeFilesFromDeployment>*.dll.config</ExcludeFilesFromDeployment>

    <!-- Redefine this list without .dll.config; see https://github.com/Microsoft/msbuild/issues/1307 -->
    <AllowedReferenceRelatedFileExtensions>.xml;.pri</AllowedReferenceRelatedFileExtensions>
  </PropertyGroup>
</Project>